{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.1",
  "description": "Block sites",
  "permissions": ["webRequest", "webRequestBlocking", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "options_page": "options.html"
}
